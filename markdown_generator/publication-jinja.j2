{% for paper in papers %}
<div style="margin-bottom: 20px;">
    <div style="color: #007bff; font-weight: bold; margin-bottom: 5px;">[{{ paper.id }}] {{ paper.title }}</div>
    <div style="margin-left: 20px; margin-bottom: 5px;">
        <span style="color: #333; font-size: 0.9em;">{{ paper.authors }}. <em>{{ paper.venue }} {{ paper.year }}</em></span>
    </div>
    {% for link in paper.links %}
    <a href="{{ link.url }}" style="margin-left: 20px; text-decoration: none; color: #007bff;">[{{ link.text }}]</a>
    {% endfor %}
</div>
{% endfor %}